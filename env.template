# Azure DevOps Personal Access Token Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control!

# Azure DevOps Personal Access Token
# Generate at: https://dev.azure.com/[organization]/_usersSettings/tokens
# Required scopes: Code (read), Project and team (read)
AZURE_DEVOPS_PAT=your_personal_access_token_here

# Azure DevOps Organization URL
# Format: https://dev.azure.com/[organization]
AZURE_DEVOPS_ORG_URL=https://dev.azure.com/your-organization

# Optional: Database connection for caching (if using)
# DATABASE_URL=sqlite:///git_analytics.db

# Optional: Logging level
LOG_LEVEL=INFO

# Optional: Output directory override
# OUTPUT_DIR=./custom_output

# Optional: Maximum concurrent API requests
MAX_CONCURRENT_REQUESTS=5

# Optional: Custom user agent for API requests
USER_AGENT=GitAnalyticsTool/1.0 

PROJECTS=[{"name":"Project1","repositories":[]}]